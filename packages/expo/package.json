{
  "name": "@bugsnag/expo",
  "version": "5.0.0-alpha.1",
  "description": "Bugsnag error reporter for Expo",
  "homepage": "https://www.bugsnag.com/",
  "author": "Bugsnag",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:bugsnag/bugsnag-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.1.1",
    "@bugsnag/core": "^5.0.0-alpha.1",
    "@bugsnag/delivery-xml-http-request": "^5.0.0-alpha.1",
    "@bugsnag/plugin-console-breadcrumbs": "^5.0.0-alpha.1",
    "@bugsnag/plugin-reactnative-errorhandler": "^5.0.0-alpha.1",
    "@bugsnag/plugin-window-unhandled-rejection": "^5.0.0-alpha.1"
  },
  "main": "index.js",
  "files": [
    "dist",
    "index.js"
  ],
  "scripts": {
    "clean": "rm -fr dist && mkdir dist",
    "build": "npm run clean && npm run build:dist",
    "build:dist": "NODE_ENV=production IS_BROWSER=yes ../../bin/bundle src/notifier.js --standalone=bugsnag | ../../bin/extract-source-map dist/bugsnag.js"
  }
}
