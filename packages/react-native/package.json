{
  "name": "@bugsnag/react-native",
  "version": "7.0.0-pre-alpha-ben.4",
  "main": "src/notifier.js",
  "types": "dist/types/bugsnag.d.ts",
  "description": "Bugsnag error reporter for React Native applications",
  "keywords": [
    "bugsnag",
    "react native",
    "exception",
    "error",
    "javascript",
    "monitoring",
    "reporting",
    "stability",
    "mobile"
  ],
  "homepage": "https://www.bugsnag.com/",
  "repository": {
    "type": "git",
    "url": "git@github.com:bugsnag/bugsnag-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "clean": "rm -fr dist && mkdir dist",
    "bundle-types": "../../bin/bundle-types",
    "build": "npm run clean && npm run bundle-types",
    "test": "nyc --reporter=lcov -- jasmine '!(node_modules)/**/*.test.js'",
    "test:types": "tslint -c ../../tslint.json types/*.d.ts && jasmine 'types/**/*.test.js'",
    "postversion": "npm run build"
  },
  "author": "Bugsnag",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "jasmine": "3.1.0",
    "nyc": "^12.0.2",
    "proxyquire": "^2.1.0",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "rnpm": {
    "ios": {
      "project": "ios/BugsnagReactNative.xcodeproj",
      "sharedLibraries": [
        "libz"
      ]
    },
    "android": {
      "packageInstance": "BugsnagReactNative.getPackage()",
      "packageImportPath": "import com.bugsnag.reactnative.BugsnagReactNative;"
    }
  },
  "dependencies": {
    "@bugsnag/core": "^6.2.0",
    "@bugsnag/plugin-console-breadcrumbs": "^6.2.0",
    "@bugsnag/plugin-react": "^6.2.0",
    "@bugsnag/plugin-react-native-app-state-breadcrumbs": "^6.2.0",
    "@bugsnag/plugin-react-native-connectivity-breadcrumbs": "^6.2.0",
    "@bugsnag/plugin-react-native-global-error-handler": "^6.2.0",
    "@bugsnag/plugin-react-native-orientation-breadcrumbs": "^6.2.0",
    "@bugsnag/plugin-react-native-unhandled-rejection": "^6.2.0",
    "bugsnag-build-reporter": "^1.0.1",
    "bugsnag-sourcemaps": "^1.1.0"
  },
  "peerDependencies": {
    "react-native": "^0.49.0",
    "react": "*"
  }
}
