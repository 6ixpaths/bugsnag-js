{
  "name": "@bugsnag/browser",
  "version": "1.0.0",
  "main": "dist/bugsnag.js",
  "types": "types/bugsnag.d.ts",
  "description": "Bugsnag error reporter for browser JavaScript",
  "homepage": "https://www.bugsnag.com/",
  "repository": {
    "type": "git",
    "url": "git@github.com:bugsnag/bugsnag-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "size": "./bin/size",
    "clean": "rm -fr dist && mkdir dist",
    "build": "npm run clean && npm run build:dist && npm run build:dist:min",
    "build:dist": "NODE_ENV=production bin/bundle | exorcist dist/bugsnag.js.map > dist/bugsnag.js",
    "build:dist:min": "NODE_ENV=production bin/bundle | bin/minify dist/bugsnag.min.js",
    "test": "bundle exec maze-runner"
  },
  "author": "Bugsnag",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@bugsnag/core": "^1.0.0",
    "@bugsnag/plugin-browser-context": "^1.0.0",
    "@bugsnag/plugin-browser-device": "^1.0.0",
    "@bugsnag/plugin-browser-request": "^1.0.0",
    "@bugsnag/plugin-browser-session": "^1.0.0",
    "@bugsnag/plugin-client-ip": "^1.0.0",
    "@bugsnag/plugin-console-breadcrumbs": "^1.0.0",
    "@bugsnag/plugin-inline-script-content": "^1.0.0",
    "@bugsnag/plugin-interaction-breadcrumbs": "^1.0.0",
    "@bugsnag/plugin-navigation-breadcrumbs": "^1.0.0",
    "@bugsnag/plugin-network-breadcrumbs": "^1.0.0",
    "@bugsnag/plugin-simple-throttle": "^1.0.0",
    "@bugsnag/plugin-strip-query-string": "^1.0.0",
    "@bugsnag/plugin-window-onerror": "^1.0.0",
    "@bugsnag/plugin-window-unhandled-rejection": "^1.0.0",
    "@bugsnag/delivery-x-domain-request": "^1.0.0",
    "@bugsnag/delivery-xml-http-request": "^1.0.0",
    "babelify": "^9.0.0",
    "browser-pack-flat": "^3.1.0",
    "browserify": "^16.2.2",
    "browserify-versionify": "^1.0.6",
    "envify": "^4.1.0",
    "exorcist": "^1.0.1",
    "jasmine": "^3.1.0",
    "nyc": "^12.0.2",
    "typescript": "^2.9.2",
    "uglify-js": "^3.4.3"
  }
}
