{
  "name": "@bugsnag/node",
  "version": "5.0.0-alpha.0",
  "main": "dist/bugsnag.js",
  "types": "types/bugsnag.d.ts",
  "description": "Bugsnag error reporter for Node.js",
  "homepage": "https://www.bugsnag.com/",
  "repository": {
    "type": "git",
    "url": "git@github.com:bugsnag/bugsnag-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "clean": "rm -fr dist && mkdir dist",
    "build": "npm run clean && npm run build:dist",
    "build:dist": "bin/bundle | exorcist dist/bugsnag.js.map > dist/bugsnag.js",
    "test": "bundle exec maze-runner"
  },
  "author": "Bugsnag",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@bugsnag/core": "^5.0.0-alpha.0",
    "@bugsnag/delivery-node": "^5.0.0-alpha.0",
    "@bugsnag/plugin-contextualize": "^5.0.0-alpha.0",
    "@bugsnag/plugin-intercept": "^5.0.0-alpha.0",
    "@bugsnag/plugin-node-device": "^5.0.0-alpha.0",
    "@bugsnag/plugin-node-in-project": "^5.0.0-alpha.0",
    "@bugsnag/plugin-node-surrounding-code": "^5.0.0-alpha.0",
    "@bugsnag/plugin-node-uncaught-exception": "^5.0.0-alpha.0",
    "@bugsnag/plugin-node-unhandled-rejection": "^5.0.0-alpha.0",
    "@bugsnag/plugin-server-session": "^5.0.0-alpha.0",
    "@bugsnag/plugin-strip-project-root": "^5.0.0-alpha.0",
    "exorcist": "^1.0.1",
    "jasmine": "^3.1.0",
    "nyc": "^12.0.2"
  },
  "dependencies": {
    "byline": "^5.0.0",
    "error-stack-parser": "^2.0.2",
    "iserror": "^0.0.2",
    "pump": "^3.0.0",
    "request": "^2.87.0",
    "stack-generator": "^2.0.3"
  }
}
