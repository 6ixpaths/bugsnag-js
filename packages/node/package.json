{
  "name": "@bugsnag/node",
  "version": "1.0.0",
  "main": "dist/bugsnag.js",
  "description": "Bugsnag error reporter for Node.js",
  "homepage": "https://www.bugsnag.com/",
  "repository": {
    "type": "git",
    "url": "git@github.com:bugsnag/bugsnag-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -fr dist && mkdir dist",
    "build": "npm run clean && npm run build:dist",
    "build:dist": "bin/bundle | exorcist dist/bugsnag.js.map > dist/bugsnag.js",
    "test": "bundle exec maze-runner"
  },
  "author": "Bugsnag",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@bugsnag/core": "^1.0.0",
    "@bugsnag/delivery-node": "^1.0.0",
    "@bugsnag/plugin-node-in-project": "^1.0.0",
    "@bugsnag/plugin-node-surrounding-code": "^1.0.0",
    "@bugsnag/plugin-server-session": "^1.0.0",
    "@bugsnag/plugin-strip-project-root": "^1.0.0",
    "babelify": "9.0.0",
    "browserify": "^16.2.2",
    "browserify-versionify": "^1.0.6",
    "exorcist": "^1.0.1",
    "jasmine": "^3.1.0",
    "nyc": "^12.0.2"
  },
  "dependencies": {
    "error-stack-parser": "^2.0.2",
    "iserror": "^0.0.2",
    "request": "^2.87.0",
    "stack-generator": "^2.0.3"
  }
}
